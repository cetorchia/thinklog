@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix thinklog: <http://localhost/carlos/thinklog/#>.

thinklog:Thought a rdfs:Class ; rdfs:label "Thought".
thinklog:Thinker a rdfs:Class ; rdfs:label "Thinker".
thinklog:Keyword a rdfs:Class ; rdfs:label "Keyword".
thinklog:CommonKeyword a rdfs:Class ; rdfs:label "CommonKeyword".
thinklog:PairCount a rdfs:Class ; rdfs:label "PairCount".

thinklog:keyword1 a rdf:Property ;
	rdfs:label "keyword1" ;
	rdfs:domain thinklog:PairCount ;
	rdfs:range thinklog:Keyword.

thinklog:keyword2 a rdf:Property ;
	rdfs:label "keyword2" ;
	rdfs:domain thinklog:PairCount ;
	rdfs:range thinklog:Keyword.

thinklog:count a rdf:Property ;
	rdfs:label "count" ;
	rdfs:domain rdf:Resource ;
	rdfs:range rdfs:Literal.

thinklog:mentions a rdf:Property ;
	rdfs:label "mentions" ;
	rdfs:domain thinklog:Thought ;
	rdfs:range thinklog:Keyword.

thinklog:relatedKeyword a rdf:Property ;
	rdfs:label "relatedKeyword" ;
	rdfs:domain thinklog:Keyword ;
	rdfs:range thinklog:Keyword .

thinklog:date a rdf:Property ;
	rdfs:label "date" ;
	rdfs:domain thinklog:Thought ;
	rdfs:range rdfs:Literal.

thinklog:thoughtId a rdf:Property ;
	rdfs:label "thoughtId" ;
	rdfs:domain thinklog:Thought ;
	rdfs:range rdfs:Literal.

thinklog:content a rdf:Property ;
	rdfs:label "content" ;
	rdfs:domain thinklog:Thought ;
	rdfs:range rdfs:Literal.

thinklog:private a rdf:Property ;
	rdfs:label "private" ;
	rdfs:domain thinklog:Thought ;
	rdfs:range rdfs:Literal.

thinklog:author a rdf:Property ;
	rdfs:label "author" ;
	rdfs:domain thinklog:Thought ;
	rdfs:range thinklog:Thinker.

thinklog:about a rdf:Property ;
	rdfs:label "about" ;
	rdfs:domain thinklog:Thinker ;
	rdfs:range rdfs:Literal.

thinklog:name a rdf:Property ;
	rdfs:label "name" ;
	rdfs:domain thinklog:Thinker ;
	rdfs:range rdfs:Literal.

thinklog:thinkerId a rdf:Property ;
	rdfs:label "thinkerId" ;
	rdfs:domain thinklog:Thinker ;
	rdfs:range rdfs:Literal.

thinklog:case1 a rdf:Resource; rdf:value "1".
thinklog:case2 a rdf:Resource; rdf:value "2".
